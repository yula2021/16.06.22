package Cafe;

public class Cafe {
    String name = "Eat it and get out";
    byte tableNumber = 15;


    public void setName(String name) {
        this.name = name;
    }

    public void setTableNumber(byte tableNumber) {
        this.tableNumber = tableNumber;
    }

    @Override
    public String toString() {
        return "Cafe{" +
                "name='" + name + '\'' +
                '}';
    }
}

